    <% include cores/header.ejs %>

    <link href='http://fonts.googleapis.com/css?family=Titillium+Web:400,300,600' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="/css/normalize.css">
    <link rel="stylesheet" href="/css/signup.css">


    <div class="row">

        <div style="float: left; width: 300px;" class="form">

            <div class="">

                <div id="login">
                    <h1>Contact Us!</h1>

                    <form action="/contact" method="post">

                        <div class="field-wrap">

                            <input type="email" name="email" required autocomplete="off" placeholder="Your Email" />
                        </div>


                        <div class="field-wrap">

                            <textarea required name="message" autocomplete="off" rows="5" placeholder="Message" ></textarea>
                        </div>


                        <button class="button button-block"/>Send</button>

                    </form>



                </div>

            </div><!-- tab-content -->

        </div> <!-- /form -->

        <div class="col-lg-4 topspace">
